{
    "name": "daim2k5/laravel-push-notification",
    "description": "Laravel package to send push notifications to mobile devices (apns, gcm)",
    "keywords": ["apns","gcm","fcm","push","notification", "laravel"],
    "authors": [
        {
            "name": "DaviBenNun",
            "email": "davi@andradenunes.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.*",
        "blablacar/notification-pusher": "v2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/blablacar/NotificationPusher"
        }
    ],
    "autoload": {
        "psr-0": {
            "Davibennun\\LaravelPushNotification": "src/"
        }
    },
    "minimum-stability": "dev"
}
